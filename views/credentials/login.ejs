<form action="/credentials/login" method="post" id="form">
    <fieldset id="fset">
        <legend id="fset">Welcome Back, Please Login Here...</legend>
        <div class="container1">
            <label for="email" id="label">Email</label>
            <input type="email" id='email' class="input" name="email" required>
        </div>
        <div class="container1">
            <label for="password" id="label">Password</label>
            <input type="password" id="password" class="input" name="password" required>
        </div>
        <div class="container1">
            <a href="/" id="caution">Cancel</a>
            <a href="/credentials/Register" id="fset">Register</a>
            <input type="submit" id="bColor" value="Login">
        </div>
    </fieldset>
</form>
