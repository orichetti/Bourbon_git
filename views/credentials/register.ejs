<form action="/credentials/register" method="post" id="form">
    <fieldset id="fset">
        <legend id="fset">Welcome, Please Register Here...</legend>
        <div class="container1">
            <label for="name" id="label">Name</label>
            <input type="name" id="name" class="input" name="name" required>
        </div>
        <div class="container1">
            <label for="email" id="label">Email</label>
            <input type="email" id="email" class="input" name="email" required>
        </div>
        <div class="container1">
            <label for="password" id="label">Password</label>
            <input type="password" id="password" class="input" name="password" required>
        </div>
        <div class="container1">
            <a href="/" id="caution">Cancel</a>
            <a href="/credentials/login" id="fset">Login</a>
            <input type="submit" id="bColor" value="Register">
    </fieldset>
</form>
<br><br><br>
